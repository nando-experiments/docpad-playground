{
  "name": "no-skeleton.docpad",
  "version": "0.1.0",
  "description": "New DocPad project without using a skeleton",
  "dependencies": {
    "docpad": "~6.79.4",
    "docpad-plugin-eco": "~2.2.1",
    "docpad-plugin-ghpages": "~2.6.1",
    "docpad-plugin-livereload": "~2.9.0",
    "docpad-plugin-sass": "~2.8.0"
  },
  "main": "node_modules/.bin/docpad-server",
  "scripts": {
    "start": "node_modules/.bin/docpad-server",
    "test": "node_modules/.bin/docpad generate --debug --silent --env static",
    "info": "node_modules/.bin/docpad info --silent"
  }
}
